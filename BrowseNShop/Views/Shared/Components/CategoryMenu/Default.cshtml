<li class="dropdown">
    <a asp-controller="Sneaker"
    asp-action="Index"
    class="dropdown-toogle" data-toggle="dropdown">Products<b class="caret"></b></a>
    <ul class="dropdown-menu">
        @foreach (var category in Model)
        {
            <li>
                <a asp-controller="Sneaker" asp-action="List"
                   asp-route-category="@category.CategoryName">@category.CategoryName</a>

            </li>
        }
        <li class="divider"></li>
        <li>
            <a asp-controller="Sneaker" asp-action="List" asp-route-category="">View all sneakers and boots</a>
        </li>

    </ul>
        
</li>