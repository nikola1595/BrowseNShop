@using BrowseNShop.Models
@model IEnumerable<Sneaker>



<div>
    <h2>All sneakers and boots are displayed here </h2>
</div>

@{ 
    foreach (var sneak in Model)
    {
        <div>
            <h4> Sneaker name: @sneak.Name</h4>
            <p>Price: @sneak.Price $</p>



        </div>
    }
}


